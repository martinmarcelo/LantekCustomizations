          <BeforePrintEvent>string workCenter = RoutingLines.Operation_WrkRef;
string operationReference = RoutingLines.Operation_OprRef;
int operationOrder = RoutingLines.Operation_OOrder;
string partReference = RoutingLines.Operation_PrdRef;
string finalString = "";

if (operationReference.ToUpper() == "2D CUT")
{
	int workCenterLength = workCenter.Length;
	if (workCenter.Substring(0,6).ToUpper() == "TRUMPF")
		operationReference = "Laser";
	else if (workCenter.Substring(workCenterLength - 7,7).ToUpper() == "AUTOGEN")
		operationReference = "Autogen";
	else if (workCenter.Substring(workCenterLength - 6,6).ToUpper() == "PLASMA")
		operationReference = "Plasma";
} 

if (operationReference.ToUpper() == "BENDING" || operationReference.ToUpper() == "BIEGEN")
{
	operationReference = "Kanten";
} 


if (operationOrder &gt; 1)
{	
	//Variable1 = OperationsPerLineDict[LinesForRouting.PrdRef];
	finalString = ", ";
	//OperationsPerLineDict[LinesForRouting.PrdRef] += ", " + LinesForRouting.Operations_WrkRef;
}

finalString += operationReference;

	//OperationsPerLineDict.Add(LinesForRouting.PrdRef, workCenter);

if (OperationsPerLineDict.ContainsKey(partReference))
{
	OperationsPerLineDict[partReference] += finalString;
}
else 
	OperationsPerLineDict.Add(partReference, finalString);</BeforePrintEvent>
