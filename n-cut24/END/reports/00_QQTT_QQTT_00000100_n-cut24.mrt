          <BeforePrintEvent>double discountPercentage = Quotes.DDisc;
double discountInvertedFactor;
double discountFinalAmount = 0;

double totalPriceParts = Quotes.Subtotal;
double transportPrice = Quotes.CarrierC;
double financialPrice = Quotes.FinancialC;
double subtotal = Quotes.Subtotal + Quotes.CarrierC + Quotes.FinancialC;

double taxes = subtotal * (Quotes.TValue / 100);

if (discountPercentage &gt; 0) 
{
	discountInvertedFactor = (1 - (Quotes.DDisc / 100));
	
	totalPriceParts = totalPriceParts / discountInvertedFactor;
	transportPrice = transportPrice / discountInvertedFactor;
	financialPrice = financialPrice / discountInvertedFactor;

	subtotal = subtotal / discountInvertedFactor;
	
	discountFinalAmount = (subtotal * (1 + (discountPercentage /100))) - subtotal;

	subtotal -= discountFinalAmount;
}
shm_Financial = financialPrice;
shm_TotalPriceParts = totalPriceParts;
shm_CarrierCosts = transportPrice;
shm_QuoteSubtotal = subtotal;
shm_Discount = discountFinalAmount;
shm_Taxes = taxes;</BeforePrintEvent>
