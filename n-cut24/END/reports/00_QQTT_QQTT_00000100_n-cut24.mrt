          <BeforePrintEvent>ForTaxBreakdown = IIF(ForTaxBreakdown != "", ForTaxBreakdown,IIF(Quotes.TValue!= Lines.TaxValue,"1","")).ToString();
if(ForPreviousTax == -5)
{
	ForNumberLineTaxes++;
	ForPreviousTax = Lines.TaxValue;
}
else if (ForPreviousTax != Lines.TaxValue)
{
	ForNumberLineTaxes++;
	//stop here, recognizing two different tax values in the Lines is enough for our purpose
}

//geometry recognition
double realArea = Lines.Items_DIS_Area;
double externalArea = Lines.Items_DIS_ExtArea;
double rectangularArea = Lines.Items_DIS_RectArea;
double widthItem = Lines.Items_DIS_Width;
double lengthItem = Lines.Items_DIS_Length;

double epsilon = 1e-6;

shm_GeometryRecognition = "";
//check if it is a rectangle
if (rectangularArea == realArea)
{	
	shm_GeometryRecognition = "&lt;br/&gt;";
	//check if it is a square
	if (widthItem == lengthItem) {
		shm_GeometryRecognition += "Quadratzuschnitt";
	} else {
		shm_GeometryRecognition += "Rechteckzuschnitt";
	}
}
//try to approximate to see if it is a circle
else if (widthItem == lengthItem)
{
	double checkRadiusPart = widthItem / 2;
	double checkCircleArea = (Math.PI * Math.Pow(checkRadiusPart,2))/1000000;

	if (Math.Abs(checkCircleArea-realArea) &lt; epsilon)
	{
		shm_GeometryRecognition = "&lt;br/&gt;";
		shm_GeometryRecognition += "Kreis D=" + widthItem.ToString("F0") + "mm";
	} 
	else if (Math.Abs(checkCircleArea-externalArea) &lt; epsilon)
	{
		shm_GeometryRecognition = "&lt;br/&gt;";
		double internalAreaScrap = externalArea - realArea;
		double internalScrapRadius = Math.Sqrt((internalAreaScrap * 1000000) / Math.PI);
	
		shm_GeometryRecognition += "Ring D=" + widthItem.ToString("F0") + "mm d=" + internalScrapRadius.ToString("F0") + "mm";
	}
}
