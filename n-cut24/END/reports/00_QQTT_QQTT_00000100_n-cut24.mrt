namespace Reports
{

    public class Report : Stimulsoft.Report.StiReport

    {
        
        public Report()
        {
            this.InitializeComponent();
        }

		public Dictionary&lt;string, string&gt; OperationsPerLineDict = new Dictionary&lt;string, string&gt;();
		public Dictionary&lt;double, double&gt; PaymentDiscountsPerTime = new Dictionary&lt;double, double&gt;();
		
		public double LeftPanelRightEndPosition = 0.0;
	
		public void RightPanelSizeAndLeftDefinition(Stimulsoft.Report.Components.StiPanel panel, double longestField, double sizeReferenceField, out double valueToIncrement, out double modifiedLeftPositionPanel)
		{				
			double usableWidthArea = ReportPage.PageWidth - (ReportPage.Margins.Left + ReportPage.Margins.Right);				
			double remainingFreeArea = usableWidthArea - LeftPanelRightEndPosition;
			double staticFieldsSize = panel.Width - sizeReferenceField;
			double totalAreaForAField = remainingFreeArea - staticFieldsSize;
			
			if (longestField &gt; totalAreaForAField)
				valueToIncrement = totalAreaForAField - sizeReferenceField;
			else 
				valueToIncrement = GetValueToGrow(longestField, sizeReferenceField);
			
			if (valueToIncrement &lt; -0.2)
				valueToIncrement = -0.2;
			
			if (LeftPanelRightEndPosition &gt; (panel.Left - valueToIncrement))
				modifiedLeftPositionPanel = LeftPanelRightEndPosition;
			else
				modifiedLeftPositionPanel = Math.Round(panel.Left,2) - Math.Round(valueToIncrement, 2);
				
		}
		
		public void LeftPanelSizeDefinition(Stimulsoft.Report.Components.StiPanel panel, double longestField, double sizeReferenceField, out double valueToIncrement)
		{
			valueToIncrement = GetValueToGrow(longestField, sizeReferenceField);
			LeftPanelRightEndPosition = panel.Left + (panel.Width + valueToIncrement);
		}
		
		public string StringChecker(string fieldContent)
		{
			if (!string.IsNullOrEmpty(fieldContent))
				return fieldContent;				
			else
				return " ";
		}
		
		
		public double GetLongestField(List&lt;KeyValuePair&lt;string, Font&gt;&gt; fieldsContent)
		{
			float dpi = 100;
			float longestField = 0.0f;
			float currentFieldSize;
			
			foreach(var field in fieldsContent)
			{
				if (!string.IsNullOrWhiteSpace(field.Key))
				{
					SizeF sizeMm = MeasureStringInMM(field.Key, field.Value, dpi);
					currentFieldSize = sizeMm.Width;
					if (currentFieldSize &gt; longestField)
					{
						longestField = currentFieldSize;
					}
				}
			}
			return longestField / 10; //to divide per 10 is needed because the value here is in mm and we need it in cm
		}
		
		public double GetValueToGrow(double longestField, double sizeReferenceField)
		{
			return (Math.Round(longestField, 1) - sizeReferenceField);
		}
		
		public SizeF MeasureStringInMM(string text, Font font, float dpi)
		{
			// creates a temporary Bitmap object only to get Graphics object
			using (Bitmap bitmap = new Bitmap(1, 1))
			{
				using (Graphics graphics = Graphics.FromImage(bitmap))
				{
					// Measures the size of the string with the font specified in pixels
					SizeF sizeInInches = graphics.MeasureString(text, font);

					// converts from pixels to mm
					float widthInMM = (sizeInInches.Width / dpi) * 25.4f;
					float heightInMM = (sizeInInches.Height / dpi) * 25.4f;

					return new SizeF(widthInMM, heightInMM);
				}
			}
		}		
		
		private const string APPLY_VAT_PARAMETER = "SYS_SAM_ApplyTaxToFinancial";
		private const string USE_PRODUCT_IMAGES = "DIS_CRM_ReportImage";
		
		public string FormatQuantity(double quantity, string quantityCategory, string weightUnit) {
			if (quantityCategory.Equals("WGH"))
				return quantity &gt;= 1000.0 ? String.Format("{0:0,0.000}", quantity) + " " + weightUnit : String.Format("{0:0.000}", quantity) + " " + weightUnit;
			else
				return String.Format("{0:0,0}", quantity);
		}
		
		public bool CheckApplyTaxToFinancialCostsParameter(bool alreadyChecked, string parName, string parValue) {
			if (alreadyChecked) return true;
			else return (parName.Equals(APPLY_VAT_PARAMETER) &amp;&amp; (parValue == "1"));
		}
		
		public bool CheckReportImageParameter(bool alreadyChecked, string parName, string parValue) {
			if (alreadyChecked) return true;
			else return (parName.Equals(USE_PRODUCT_IMAGES) &amp;&amp; (parValue == "1"));
		}
		
		public double AddCarrierAndFinancialVATAmount(double subTotal, double carrierAmount, double financialAmount, double lineTaxValue, double taxValue, bool addFinancialCostsToVAT) {
			double result = subTotal;
			if(lineTaxValue == taxValue) { 
				// Carrier amount is added in all cases
				result += carrierAmount;
				// Financial costs VAT is only added when configuration parameter sets so
				if (addFinancialCostsToVAT) result += financialAmount;
			}	
			return result;
		}
		
		public double CalculateVATAmount(double subTotal, double carrierAmount, double financialAmount, double lineTaxValue, double taxValue, bool addFinancialCostsToVAT) {
			double result = subTotal;
			if(lineTaxValue == taxValue) { 
				// Carrier amount is added in all cases
				result += CalculateVATAmount(carrierAmount, taxValue);
				// Financial costs VAT is only added when configuration parameter sets so
				if (addFinancialCostsToVAT) result += CalculateVATAmount(financialAmount, taxValue);
			}	
			return result;	
		}
		public double CalculateVATAmount( double amount, double taxValue) {
			return (amount * (taxValue / 100));
		}
		
		public double CalculateVATAmount (double carrierAmount, double financialAmount, double taxValue, bool addFinancialCostsToVAT) {
			// Carrier amount is added in all cases
			double result = carrierAmount;
			// Financial costs VAT is only added when configuration parameter sets so
			if (addFinancialCostsToVAT) result += financialAmount;
			return CalculateVATAmount(result, taxValue);
		}
		
		public bool CheckCarrierVAT(bool forCheckCarrierVATCheck, double lineTaxValue, double carrierAmount, double carrierTaxValue) {
			if (forCheckCarrierVATCheck) return true;
			else if ((lineTaxValue == carrierTaxValue) &amp;&amp; (carrierAmount != 0.0)) return true;
			else return false;
		}
		
		public bool CheckFinancialVAT(bool forCheckFinancialVAT, bool addFinancialCostsToVAT, double lineTaxValue, double financialAmount, double financialTaxValue) {
			if ((forCheckFinancialVAT) || (!addFinancialCostsToVAT)) return true;
			else if ((lineTaxValue == financialTaxValue) &amp;&amp; (financialAmount != 0.0)) return true;
			else return false;
		}
		
